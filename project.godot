config_version=4

[application]

config/name="Atom Assembler"
config/description="A game about assembling atoms to make molecules.
Made for Godot Wild Jam #53"
run/main_scene="res://TitleScreen.tscn"
boot_splash/image="res://loading.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.0784314, 0.133333, 0.25098, 1 )
config/icon="res://assets/1.png"
config/windows_native_icon="res://assets/icon.ico"

[autoload]

Save="*res://Save.gd"
Music="*res://Music.tscn"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/jamcountdown/plugin.cfg" )

